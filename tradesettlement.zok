def main(field private sellerPrice, field private buyerBid, field private marketCap, field tradeAmount) -> (field, field){

    field res = sellerPrice <= buyerBid && tradeAmount <= marketCap ? 1 : 0;

    field finalMedianPrice = (sellerPrice + buyerBid) / 2;

   return ( res , finalMedianPrice ) ;
}
    
   
